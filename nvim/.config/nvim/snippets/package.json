{
  "name": "custom-frontend-snippets",
  "version": "1.0.0",
  "description": "Custom snippets for frontend development",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./react.json"
      },
      {
        "language": "typescriptreact",
        "path": "./react-testing.json"
      },
      {
        "language": "javascript",
        "path": "./typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./react.json"
      },
      {
        "language": "javascriptreact",
        "path": "./react-testing.json"
      },
      {
        "language": "css",
        "path": "./css.json"
      },
      {
        "language": "html",
        "path": "./html.json"
      },
      {
        "language": "json",
        "path": "./json.json"
      }
    ]
  }
}
